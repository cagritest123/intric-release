# Docker Registry Configuration
NEXUS_REGISTRY=your.nexus.registry.com
IMAGE_TAG=latest

# Network Configuration
SERVICE_FQDN_FRONTEND=example.com
FRONTEND_PORT=3000
BACKEND_PORT=8123

# Database Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_PORT=5432
POSTGRES_DB=postgres

# Redis Configuration
REDIS_PORT=6379

# API Keys and Model URLs
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
AZURE_API_KEY=
AZURE_MODEL_DEPLOYMENT=
AZURE_ENDPOINT=
AZURE_API_VERSION=

# Authentication
JWT_SECRET=your_secure_jwt_secret_here
JWT_AUDIENCE=*
JWT_ISSUER=EXAMPLE
JWT_EXPIRY_TIME=86000
JWT_ALGORITHM=HS256
JWT_TOKEN_PREFIX=

# Max Sizes
UPLOAD_FILE_TO_SESSION_MAX_SIZE=1048576
UPLOAD_IMAGE_TO_SESSION_MAX_SIZE=1048576
UPLOAD_MAX_FILE_SIZE=10485760
TRANSCRIPTION_MAX_FILE_SIZE=10485760
MAX_IN_QUESTION=1

# Feature Flags
USING_ACCESS_MANAGEMENT=False
USING_AZURE_MODELS=False

# Security
API_PREFIX=/api/v1
API_KEY_LENGTH=64
API_KEY_HEADER_NAME=example

# Optional Frontend Settings
MOBILITY_GUARD_AUTH=
SHOW_TEMPLATES=
FEEDBACK_FORM_URL=

# Optional Authentication Dependencies
MOBILITYGUARD_DISCOVERY_ENDPOINT=
MOBILITYGUARD_CLIENT_ID=
MOBILITYGUARD_CLIENT_SECRET=

# Logging
LOGLEVEL=INFO
